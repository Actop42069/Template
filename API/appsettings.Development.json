{
  "ConnectionStrings": {
    "Database": "server=[SQLSERVER_NAME];database=[DATABASE_NAME];Trusted_Connection=true;TrustServerCertificate=Yes;Encrypt=False"
  },
  "JWT": {
    "Audience": "[AUDIENCE_URL]",
    "Issuer": "[ISSUER_URL]",
    "ExpireInMinutes": 60,
    "Key": "[LONG_SECRET_KEY]"
  },
  "SMTP": {
    "Host": "[SMTP_HOST]",
    "Port": 25,
    "Sender": "[SENDER_EMAIL]"
  },
  "Application": {
    "ClientUrl": "[CLIENT_URL]",
    "AdminUrl": "[ADMIN_URL]",
    "ApiUrl": "[API_URL]"
  },
  "CORS": {
    "AllowedOrigins": [ "[ALLOWED_ORIGIN]" ],
    "ExposedHeaders": [ "[HEADER1]", "[HEADER2]", "[HEADER3]" ]
  },
  "FTP": {
    "Hostname": "[FTP_HOSTNAME]",
    "Username": "[FTP_USERNAME]",
    "Password": "[FTP_PASSWORD]"
  }
}
